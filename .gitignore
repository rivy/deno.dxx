# ignore intermediate/undistributed build artifacts
build
[._@#$]build
target

# ignore coverage data
coverage
[._@#$]coverage
.nyc_output

# ignore Deno and JS import/package-related files
deno.lock
node_modules
package-lock.json
yarn.lock
# * allow packaging of lock files into '.deps-lock'
!.deps-lock/deno.lock
!.deps-lock/package-lock.json
!.deps-lock/yarn.lock
